   "source": [
    "# 1. check if the given string is a palindrome\n",
    "\n",
    "\n",
    "def ispalindrome(string):\n",
    "    if string == string[::-1]:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "    \n",
    "string = \"malayalam\"\n",
    "\n",
    "result = ispalindrome(string)\n",
    "print(result)"
   ]
